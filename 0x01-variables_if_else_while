#include <stdlib.h>                                                                                        
#include <time.h>                                                                                          
#include <stdio.h>
/** 
* main - main block
* description: get a random number and print number
* return 0
*/
int main(void)
{
int n;
srand(time(o));
n = rand -RAND_MAX /2;
if (n >0)
print("%i is positive\n", n);
else if n < o)
printf("i is negative\n", n);
else
printf("%i is zero\n", n);
return(0);
}

